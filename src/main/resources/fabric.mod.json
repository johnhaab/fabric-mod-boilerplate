{
	"schemaVersion": 1,
	"id": "example-mod",
	"version": "${version}",
	"name": "Example Mod",
	"description": "This is a boilerplate for Fabric mod development.",
	"authors": [
		"keziumo"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/example-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"xyz.johnhaab.example.ExampleMod"
		],
		"client": [
			"xyz.johnhaab.example.ExampleModClient"
		],
		"fabric-datagen": [
			"xyz.johnhaab.example.ExampleModDataGenerator"
		]
	},
	"mixins": [
		"testing-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}